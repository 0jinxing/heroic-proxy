{
  "manifest_version": 2,
  "name": "[DEVELOPMENT] Onion",
  "description": "A simple proxy tool",
  "version": "1.2.0",
  "permissions": ["proxy", "storage", "tabs", "http://*/*", "https://*/*"],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "64": "icon.png"
  },
  "options_page": "index.html",
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": true
  }
}
