{
  "manifest_version": 2,
  "name": "Oh Proxy",
  "description": "a simple proxy tool",
  "version": "1.0.0",
  "permissions": ["proxy", "storage", "tabs", "windows"],
  "browser_action": {
    "default_icon": "proxy-enable.png"
  },
  "icons": {
    "64": "proxy-enable.png"
  },
  "options_page": "index.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
